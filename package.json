{
  "name": "nation-fun",
  "version": "1.0.0",
  "description": "A project to connect and create agents.",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:bdd": "cucumber-js features/e2e --require features/step_definitions/authentication_steps.cjs --format json:reports/cucumber_report.json",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "test:e2e": "npm run build && start-server-and-test 'npm start' http://localhost:3000 'npm run test:bdd'",
    "generate-report": "node generate-report.js",
    "agents:sync": "tsx scripts/sync-agents.ts",
    "knip": "knip"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "knip"
    ]
  },
  "dependencies": {
    "@module-federation/nextjs-mf": "^8.8.49",
    "@privy-io/react-auth": "^3.6.1",
    "@ramp-network/ramp-instant-sdk": "^6.2.0",
    "@solana-program/system": "^0.10.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "axios": "^1.6.0",
    "ethers": "^6.15.0",
    "jest-environment-jsdom": "^30.2.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "^5.103.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.12.12",
    "@types/react": "19.2.7",
    "cucumber-html-reporter": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "knip": "^5.70.2",
    "lint-staged": "^16.2.7",
    "nock": "^13.3.0",
    "sinon": "^15.0.0",
    "start-server-and-test": "^2.1.2",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
