{
  "name": "nation-fun",
  "version": "1.0.0",
  "description": "A project to connect and create agents.",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "pretest:bdd": "tsc -p tsconfig.bdd.json && cp -R features/ dist-test/ && cp -R services/ dist-test/",
    "test:bdd": "cucumber-js dist-test --format json:reports/cucumber_report.json",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "test:e2e": "npm run build && start-server-and-test 'npm start' http://localhost:3000 'npm run test:bdd'",
    "generate-report": "node generate-report.js",
    "agents:sync": "tsx scripts/sync-agents.ts",
    "knip": "knip"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "knip"
    ]
  },
  "dependencies": {
    "@privy-io/react-auth": "^3.6.1",
    "@ramp-network/ramp-instant-sdk": "^6.2.0",
    "@solana-program/system": "^0.10.0",
    "axios": "^1.6.0",
    "ethers": "^6.15.0",
    "next": "^14.0.0",
    "node-cron": "^4.2.1",
    "pino": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "twitter-api-v2": "^1.28.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.0.0",
    "@types/jest": "^30.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/sinon": "^17.0.3",
    "copyfiles": "^2.4.1",
    "cucumber-html-reporter": "^7.2.0",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "knip": "^5.70.2",
    "lint-staged": "^16.2.7",
    "nock": "^13.3.0",
    "sinon": "^15.0.0",
    "start-server-and-test": "^2.1.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.2.6",
    "typescript": "^5.9.3"
  }
}
